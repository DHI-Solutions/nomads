{
  "name": "@dhi-solutions/mapbox",
  "version": "0.0.3",
  "side-effects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p tsconfig.json",
    "build:js": "cross-env BABEL_ENV=production babel --root-mode upward src --out-dir dist --extensions .ts,.tsx --no-comments",
    "build:types": "tsc -p tsconfig.build.json",
    "prebuild": "yarn typecheck",
    "build": "yarn build:js --delete-dir-on-start ",
    "postbuild": "yarn build:types",
    "watch": "run-p \"build:types -w\" \"build:js --watch\""
  },
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@deck.gl/aggregation-layers": "7.3.3",
    "@deck.gl/core": "7.3.3",
    "@deck.gl/geo-layers": "7.3.3",
    "@deck.gl/layers": "7.3.3",
    "@deck.gl/mesh-layers": "7.3.3",
    "@deck.gl/react": "7.3.3",
    "d3-ease": "1.0.6",
    "deck.gl": "7.3.3",
    "mapbox-gl": "1.6.1",
    "react-map-gl": "5.2.1",
    "slugify": "^1.3.6"
  },
  "devDependencies": {
    "@types/d3-ease": "1.0.9",
    "@types/geojson": "7946.0.7",
    "@types/react-map-gl": "5.1.0"
  }
}
